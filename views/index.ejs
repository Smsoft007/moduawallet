<script>
  init()
  function init() {
    addresies.forEach((item, i) => {
      var innerHtml = '';
      if (item.ADDR != "") {
        // commonLib.getCoinAmount(null, addresies[1].ADDR).then(data => {
        //
        // })
        innerHtml = '<h6 class="color-white text-right pl-4 break-word">' + item.ADDR + ' &nbsp;<a href="#" class="icon color-green-dark"><i class="far fa-copy" onclick="commonLib.copyText(\'' + item.ADDR + '\')"></i></a></h6>';
        commonLib.getCoinAmount((item.COINNAME == "ICK") ? "0x8177ef6a05adba53cb78ca4da6f4a66766bcf69d" : null, item.ADDR).then(data => {
          BINDVALUEDATA[item.COINNAME + "Amount"] = data.AMOUNT
          BINDVALUEDATA[item.COINNAME + "Price"] = data.RATE * data.AMOUNT
          BINDVALUEDATA[item.COINNAME + "Rate"] = data.RATE
          commonLib.bindValueNameData(BINDVALUEDATA)
        })
        if (item.COINNAME == "ICK") {
          commonLib.getCoinAmount(null, item.ADDR).then(data => {
            $("DATA-TAG[name=coinFeeVal]").text(data.AMOUNT)
          })
        }
      } else {
        innerHtml = '<h6 class="text-right pl-4 break-word"><a data-menu="menu-charge" class="btn btn-border btn-xs rounded-sm text-uppercase font-900 border-green-dark color-green-dark" onclick="changeSelecedCoin(' + i +
          ')">+ <%=LANG['LANG262'][NUM]%></a></h6>';
      }
      $("#" + item.COINNAME).append(innerHtml)
    });
    reqApiList({
      URL: 'getPendinglist'
    }).then(data => {
      var innerHtml = '';
      $("#pendList").empty()
      if (data.PENDINGLIST[0].length == 0) {
        innerHtml += '<a href="#">';
        innerHtml += '<p class="text-center"><%=LANG['LANG261'][NUM]%></p>';
        innerHtml += '</a>';
      }
      data.PENDINGLIST[0].forEach(item => {
        innerHtml += '<a>';
        innerHtml += '<div class="spinner-border color-blue-dark mr-2" role="status">';
        innerHtml += '<span class="sr-only">Loading...</span>';
        innerHtml += '</div>';
        innerHtml += '<span>' + item.RECIVEADDR + '</span>';
        innerHtml += '<strong class="break-word">TXID : ' + item.TXID + '</strong>';
        innerHtml += '<span class="badge bg-red-dark font-11">' + item.MS_QTY + ' ' + item.MS_SNAME + '</span>';
        innerHtml += '<i class="fa fa-angle-right"></i>';
        innerHtml += '</a>';
      });
      $("#pendList").append(innerHtml)
    })
  }

  // function changeSelecedCoin(i) {
  //   selectedCoin = i
  //   console.log(selectedCoin);
  // }
  $('.card-slider').owlCarousel({
